<div class="container ">
<div class="row">
  <div class="col-sm-3"></div>
  <div class="col-sm-6 landing-content-area">
    <h3>rfp response</h3>
    <%= bootstrap_form_for(@rfp_answer) do |f| %>
      <% @rfp_request.each do |rfp| %>
        <h6><%= rfp.question %><h6>
        <%= f.text_area :answer %>
      <%end%>
      <%= f.submit "Submit" , {class: "btn btn-primary js-scroll-trigger"} %>
    <%end%>
  </div>
  <div class="col-sm-3"></div>
</div>
</div>